{
  "name": "angular-template",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build -c production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "autoversion": "node ./semanticVersion.js",
    "coverage": "ng test --code-coverage --watch=false",
    "prettier": "prettier --check",
    "release": "standard-version --tag-prefix '' -a",
    "quality": "npm run coverage && node_modules/sonar-scanner/bin/sonar-scanner",
    "rename": "node ./renameProject.js",
    "gitlab": "npm i && npm run lint && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.8",
    "@angular/cdk": "11.2.7",
    "@angular/common": "11.2.8",
    "@angular/compiler": "11.2.8",
    "@angular/core": "11.2.8",
    "@angular/forms": "11.2.8",
    "@angular/material": "11.2.7",
    "@angular/platform-browser": "11.2.8",
    "@angular/platform-browser-dynamic": "11.2.8",
    "@angular/router": "11.2.8",
    "replace-in-file": "6.3.5",
    "rxjs": "6.6.3",
    "tslib": "2.0.1",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.7",
    "@angular/cli": "11.2.7",
    "@angular/compiler-cli": "11.2.8",
    "@types/jasmine": "3.6.0",
    "@types/node": "12.11.1",
    "codelyzer": "^6.0.0",
    "cz-conventional-changelog": "3.3.0",
    "git-cz": "4.7.6",
    "husky": "4.3.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.0",
    "karma": "6.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.0",
    "typescript": "4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "./**/*.{ts,js,json,scss}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ng lint",
      "pre-push": "ng build -c production && npm run quality"
    }
  }
}
